@keyframes pulse 
  0%
    transform scale(1) 	
	50%
	  transform scale(1.1) 
  100%
    transform scale(1) 


.pulse 
	-webkit-animation-name pulse
	-moz-animation-name pulse
	-ms-animation-name pulse
	-o-animation-name pulse
	animation-name pulse
