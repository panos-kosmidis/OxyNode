@keyframes rotateOut 
	0% 
		transform-origin center center
		transform rotate(0)
		opacity 1
	
	
	100% 
		transform-origin center center
		transform rotate(200deg)
		opacity 0
	


.rotateOut 
	-webkit-animation-name rotateOut
	-moz-animation-name rotateOut
	-ms-animation-name rotateOut
	-o-animation-name rotateOut
	animation-name rotateOut
